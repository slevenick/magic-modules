def identifiers
<% params = extract_identifiers(format_url(object.self_link_url)) -%>
<% params_word_arr = "%w{#{params.join(' ')}}" -%>
  params = <%= params_word_arr %>

  result = []
  @table.each do |item|
    combo = item.merge(@params)
    item_identifiers = {}
    params.each do |param|
      item_identifiers[param.to_sym] = combo[param.to_sym]
    end
    result.push(item_identifiers)
  end
  result
end